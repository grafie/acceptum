{
  "name": "acceptum",
  "version": "0.2.1",
  "manifest_version": 2,
  "homepage_url": "https://github.com/grafie/acceptum/",
  "description": "Quickly capture and automatically organize your recipts, your web research, or anything you browse!",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "64": "icons/64.png"
  },
  "author": "Poe Rhiza",
  "permissions": [
    "notifications",
    "activeTab",
    "<all_urls>",
    "downloads",
    "storage"
  ],
  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_title": "Save Page",
    "default_popup": "browser_action/save-page.html"
  },
  "applications": {
    "gecko": {
      "id": "{88888888-4444-4444-4444-121212121212}",
      "update_url": "https://github.com/grafie/acceptum/raw/master/firefox/dist/updates.json"
    }
  }
}
